{
  "name": "tempo",
  "version": "1.0.0",
  "repository": "https://github.com/fponticelli/tempots.git",
  "author": "Franco Ponticelli <franco.ponticelli@gmail.com>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:demos": "turbo build --filter=./demo/*",
    "build:watch": "turbo build:watch",
    "docs": "turbo docs && yarn workspace @tempos/docs docs:prep",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "test:coverage": "turbo test:coverage",
    "npm:publish": "turbo npm:publish"
  },
  "workspaces": [
    "apps/*",
    "demo/*",
    "packages/*"
  ],
  "devDependencies": {
    "turbo": "^2.1.2"
  },
  "packageManager": "yarn@1.22.19"
}