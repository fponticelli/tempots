{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.50.0",
  "pnpmVersion": "6.10.3",
  "pnpmOptions": {
    "strictPeerDependencies": true
  },
  "projectFolderMaxDepth": 3,
  "gitPolicy": {},
  "repository": {
    "url": "git@github.com:fponticelli/tempo2.git"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "projects": [
    {
      "packageName": "tempo-dom",
      "projectFolder": "./dom",
      "shouldPublish": true
    },
    {
      "packageName": "tempo-demo.readme",
      "projectFolder": "./demo/readme",
      "shouldPublish": false
    }
  ],
  "ensureConsistentVersions": true
}