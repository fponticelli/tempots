{
  "name": "@tempots/demo-todomvc",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist && rm -f *.tsbuildinfo",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "compile": "node esbuild.js",
    "build": "npm run typecheck && npm run clean && npm run compile && cp src/index.html dist/",
    "start": "npm run clean && mkdir dist && cp src/index.html dist/ && node esbuild.js --server",
    "test": "echo 'no tests'",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0",
    "format": "prettier --config ../../.prettierrc 'src/**/*.{ts,tsx}' --write"
  },
  "type": "module",
  "dependencies": {
    "@tempots/std": "*",
    "@tempots/dom": "*"
  },
  "devDependencies": {
    "todomvc-common": "^1.0.5",
    "todomvc-app-css": "^2.4.2",
    "@ts-stack/markdown": "^1.4.0",
    "@types/dompurify": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@uppercod/esbuild-jsx-runtime": "^1.1.1",
    "dompurify": "^2.3.10",
    "esbuild": "^0.14.51",
    "esbuild-sass-plugin": "^2.3.1",
    "eslint": "^8.20.0",
    "prettier": "^2.7.1",
    "superstatic": "^8.0.0",
    "typescript": "^4.7.4"
  }
}
