{
  "name": "@tempots/demo-readme",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist && rm -f *.tsbuildinfo",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "compile": "node esbuild.js",
    "build": "npm run typecheck && npm run clean && npm run compile && cp src/index.html dist/",
    "start": "node esbuild.js --server",
    "test": "echo 'no tests'",
    "lint": "eslint 'src/**/*.{ts,tsx}' --max-warnings=0",
    "format": "prettier --config ../../.prettierrc 'src/**/*.{ts,tsx}' --write"
  },
  "type": "module",
  "dependencies": {
    "@tempots/std": "*",
    "@tempots/dom": "*"
  },
  "devDependencies": {
    "@uppercod/esbuild-jsx-runtime": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "esbuild": "^0.14.51",
    "eslint": "^8.20.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
